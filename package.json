{
  "name": "Nuvem42",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./src/bin/www",
    "dev:server": "cross-env NODE_ENV=development nodemon ./src/bin/www",
    "dev:client": "npm run serve --prefix client",
    "install:client": "npm install --prefix client",
    "build:client": "npm run build --prefix client",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "pretest": "cross-env NODE_ENV=test yarn sequelize-cli db:migrate",
    "posttest": "cross-env NODE_ENV=test yarn sequelize-cli db:migrate:undo:all"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "debug": "~4.3.1",
    "dotenv": "^9.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "~1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.0",
    "nodemailer-mailgun-transport": "^2.0.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "socket.io": "^4.0.2",
    "validator": "^13.6.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.26.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "factory-girl": "^5.0.4",
    "faker": "^5.5.3",
    "jest": "^26.6.3",
    "jest-environment-node": "^26.6.2",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "sqlite3": "^5.0.3",
    "supertest": "^6.1.3"
  }
}
